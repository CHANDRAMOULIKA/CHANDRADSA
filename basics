import java.util.*;
class Main {
    public static void main(String[] args) {
        System.out.println("Enter Input");
        Scanner scan= new Scanner(System.in);
        int age= scan.nextInt();
        if(age>=18){
            System.out.println("Eligible to vote");
          }  else{
                System.out.println("Not Eligible to vote");
            }
        }
    }
